<Window x:Class="HBBio.Chromatogram.MarkerDelWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HBBio.Chromatogram"
        mc:Ignorable="d"
        Title="{DynamicResource Ch_MarkerDelWin}" Window.SizeToContent="WidthAndHeight" MaxHeight="300" MinWidth="300" WindowStartupLocation="CenterScreen">
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="5"/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="10"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="listBox" Grid.ColumnSpan="4"/>
        <Button x:Name="btnOK" Grid.Column="1" Grid.Row="2" Content="{DynamicResource btnOK}" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Grid.Column="3" Grid.Row="2" Content="{DynamicResource btnCancel}" Click="btnCancel_Click"/>
    </Grid>
</Window>
