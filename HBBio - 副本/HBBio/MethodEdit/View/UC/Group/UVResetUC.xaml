<UserControl x:Class="HBBio.MethodEdit.UVResetUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HBBio.MethodEdit"
             mc:Ignorable="d">
    <GroupBox Header="{DynamicResource ME_EnumGroupType_UVReset}">
        <CheckBox Margin="15" VerticalAlignment="Top" Content="{DynamicResource ME_ResetUVMonitor}" IsChecked="{Binding MEnableResetUV}"/>
    </GroupBox>
</UserControl>
